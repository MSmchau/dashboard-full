{
  "name": "dashboard-full",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "可视化大屏数据展示系统 - 基于Vue 3 + TypeScript + Vite",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "preview:prod": "vite preview --host 0.0.0.0 --port 3000",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run src/test/integration/",
    "test:unit": "vitest run src/services/__tests__/ src/views/__tests__/",
    "serve": "node server.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@langchain/community": "^0.2.0",
    "@langchain/openai": "^0.2.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/xlsx": "^0.0.35",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "countup.js": "^2.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.11.7",
    "esbuild": "^0.27.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "langchain": "^0.2.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "three": "^0.158.0",
    "vue": "^3.4.0",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.2.5",
    "vue3-countup": "^0.0.4",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.1",
    "@rushstack/eslint-patch": "^1.15.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.0",
    "@types/three": "^0.158.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "dashboard",
    "visualization",
    "monitoring",
    "vue3",
    "typescript",
    "echarts",
    "three.js"
  ],
  "author": {
    "name": "Dashboard Team",
    "email": "dashboard@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dashboard-full.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/dashboard-full/issues"
  },
  "homepage": "https://github.com/your-org/dashboard-full#readme"
}
